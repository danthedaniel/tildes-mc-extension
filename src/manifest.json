{
  "manifest_version": 2,
  "name": "Tildes MC Status Indicators",
  "version": "1.0.1",
  "description": "Appends DynMap links to usernames on tildes.net",
  "icons": {
    "32": "icons/icon32.png"
  },
  "permissions": [
    "https://tildes.net/*",
    "https://tildes.nore.gg/standalone/MySQL_update.php"
  ],
  "content_scripts": [
    {
      "matches": ["https://tildes.net/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
